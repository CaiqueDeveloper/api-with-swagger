                    "Authenticate"
                ],
                "summary": "Login User",
                "operationId": "d69da90a82dd646cb7204eeab335e2a8",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "description": "User email",
                                        "type": "string"
                                    },
                                    "password": {
                                        "description": "User password",
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            },
                            "example": {
                                "email": "userteste@email.com",
                                "password": "user_teste_pass"
                            }
                        }
                    }
                },
                "responses": {
                    "202": {
